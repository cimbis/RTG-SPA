{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "classes": true
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": [
        "jest"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended"
    ]
}